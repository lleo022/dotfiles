{
  "cheatsheetData": [
    {
      "title": "Applications",
      "binds": [
        {
          "keys": "Super + Return",
          "desc": "Terminal"
        },
        {
          "keys": "Super + Space",
          "desc": "App Launcher"
        },
        {
          "keys": "Super + Shift + W",
          "desc": "Wallpaper Selector"
        },
        {
          "keys": "Super + Shift + Slash",
          "desc": "Keybinds List"
        }
      ]
    },
    {
      "title": "Window Management",
      "binds": [
        {
          "keys": "Super + Q",
          "desc": "Close Window"
        },
        {
          "keys": "Super + F",
          "desc": "Maximize"
        },
        {
          "keys": "Super + Shift + F",
          "desc": "Fullscreen"
        },
        {
          "keys": "Super + Ctrl + F",
          "desc": "Expand Column"
        },
        {
          "keys": "Super + V",
          "desc": "Toggle Floating"
        },
        {
          "keys": "Super + Shift + V",
          "desc": "Swap Floating / Tiling Focus"
        },
        {
          "keys": "Super + W",
          "desc": "Toggle Tabbed Column"
        },
        {
          "keys": "Super + BracketLeft",
          "desc": "Consume / Expel Left"
        },
        {
          "keys": "Super + BracketRight",
          "desc": "Consume / Expel Right"
        },
        {
          "keys": "Super + Comma",
