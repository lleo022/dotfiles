      "binds": [
        {
          "keys": "Super + C",
          "desc": "Center Column"
        },
        {
          "keys": "Super + Ctrl + C",
          "desc": "Center Visible Columns"
        },
        {
          "keys": "Super + R",
          "desc": "Cycle Column Width"
        },
        {
          "keys": "Super + Shift + R",
          "desc": "Cycle Window Height"
        },
        {
          "keys": "Super + Ctrl + R",
          "desc": "Reset Window Height"
        },
        {
          "keys": "Super + Minus",
          "desc": "Column Width -10%"
        },
        {
          "keys": "Super + Equal",
          "desc": "Column Width +10%"
        },
        {
          "keys": "Super + Shift + Minus",
          "desc": "Window Height -10%"
        },
        {
          "keys": "Super + Shift + Equal",
          "desc": "Window Height +10%"
        }
      ]
    },
    {
      "title": "Focus Navigation (Arrows + IJKL)",
      "binds": [
        {
          "keys": "Super + Tab",
          "desc": "Toggle Overview"
        },
        {
          "keys": "Super + I",
          "desc": "Focus Window Up"
        },
        {
          "keys": "Super + K",
          "desc": "Focus Window Down"
        },
        {
          "keys": "Super + J",
          "desc": "Focus Column Left"
        },
        {
          "keys": "Super + L",
          "desc": "Focus Column Right"
        },
        {
          "keys": "Super + Home",
          "desc": "Focus Column First"
        },
        {
          "keys": "Super + End",
          "desc": "Focus Column Last"
        }
      ]
    },
    {
      "title": "Moving Windows (Arrows + IJKL)",
      "binds": [
        {
          "keys": "Super + Shift + I",
          "desc": "Move Window Up"
