        },
        {
          "keys": "Super + Period",
          "desc": "Expel Window from Column"
        }
      ]
    },
    {
      "title": "Screenshots",
      "binds": [
        {
          "keys": "Super + Shift + S",
          "desc": "Screenshot"
        },
        {
          "keys": "Super + Ctrl + S",
          "desc": "Screenshot Screen"
        },
        {
          "keys": "Super + Alt + S",
          "desc": "Screenshot Window"
        }
      ]
    },
    {
      "title": "System",
      "binds": [
        {
          "keys": "Super + Escape",
          "desc": "Lock Screen"
        },
        {
          "keys": "Super + M",
          "desc": "Power Menu"
        },
        {
          "keys": "Super + Shift + M",
          "desc": "Quit"
        },
        {
          "keys": "Ctrl + Alt + Delete",
          "desc": "Quit"
        },
        {
          "keys": "Super + Shift + P",
          "desc": "Power Off Monitors"
        }
      ]
    },
